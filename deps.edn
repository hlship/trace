{:deps {org.clojure/clojure    {:mvn/version "1.11.2"}
        org.clj-commons/pretty {:mvn/version "2.3.0"}
        criterium/criterium    {:mvn/version "0.4.6"}}
 :aliases
 {;; clj -X:test
  :test
  {:extra-paths ["test"]
   :extra-deps  {io.github.cognitect-labs/test-runner {:git/tag "v0.5.0"
                                                       :git/sha "b3fd0d2"}}
   :exec-fn     cognitect.test-runner.api/test}

  :nrepl
  {:extra-deps {nrepl/nrepl {:mvn/version "1.1.0"}}
   :main-opts ["-m" "nrepl.cmdline"]}

  ; clj -T:build
  :build
  {:deps       {io.github.hlship/build-tools {:git/tag "0.10.1" :git/sha "7ecff5b"}}
   :ns-default build}}

 :codox/config
 {:description "Utility to assist with REPL-oriented debugging"
  :source-uri  "https://github.com/hlship/trace/blob/master/{filepath}#L{line}"}}
